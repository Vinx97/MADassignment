        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.38" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="117dp"
        android:layout_height="35dp"
        android:layout_marginBottom="8dp"
