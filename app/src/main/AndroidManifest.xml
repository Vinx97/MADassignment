<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.tarc.madassignment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QRCodeActivity" />
        <activity android:name=".subjects_enrolled" />
        <activity
            android:name=".studentActivity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".studentMenuActivity"
            android:label="@string/title_activity_student_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".cameraActivity" />
        <activity
            android:name=".teacherMenuActivity"
            android:label="@string/title_activity_teacher_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".teacherActivity"
            android:label="@string/title_activity_teacher"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>